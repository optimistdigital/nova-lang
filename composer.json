{
  "name": "optimistdigital/nova-lang",
  "description": "A Laravel Nova tool.",
  "keywords": [
    "laravel",
    "nova",
    "optimistdigital"
  ],
  "authors": [
    {
      "name": "Kaspar Rosin",
      "email": "kaspar@optimistdigital.com",
      "role": "Developer"
    },
    {
      "name": "Optimist Digital",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\NovaLang\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\NovaLang\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
